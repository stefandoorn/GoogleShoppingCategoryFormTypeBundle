{
    "name": "stefandoorn/google-shopping-category-form-type-bundle",
    "description": "Form type to select a Google Shopping Category ID",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.4 || ^8.1",
        "symfony/form": "^4.0 || ^5.0",
        "symfony/framework-bundle": "^4.0 || ^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Doorn",
            "email": "stefan@efectos.nl"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "StefanDoorn\\GoogleShoppingCategoryFormTypeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StefanDoorn\\GoogleShoppingCategoryFormTypeBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "sylius-labs/coding-standard": "^4.3",
        "symplify/easy-coding-standard": "^11.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
