{
    "name": "stefandoorn/google-shopping-category-form-type-bundle",
    "description": "Form type to select a Google Shopping Category ID",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.3",
        "symfony/form": "^4.0|^5.0",
        "symfony/framework-bundle": "^4.0|^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Doorn",
            "email": "stefan@efectos.nl"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "StefanDoorn\\GoogleShoppingCategoryFormTypeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StefanDoorn\\GoogleShoppingCategoryFormTypeBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "phpstan/phpstan": "^0.12.59",
        "phpunit/phpunit": "^9.5",
        "sylius-labs/coding-standard": "^3.0",
        "symplify/easy-coding-standard": "^8.3"
    }
}
